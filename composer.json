    "type": "project",
    "homepage": "https://github.com/barrydit/codeinsync",
    "require": {
        "php": "^8.1 || ^8.2.5",
        "composer/composer": "^2.7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "license": "Unlicensed",
    "authors": [
        {
            "name": "Barry Dick",
            "email": "barryd.it@gmail.com"
        }
